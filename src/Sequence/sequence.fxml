<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainVbox" prefHeight="700.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Sequence.SequenceController">
   <children>
      <AnchorPane prefHeight="63.0" prefWidth="400.0">
         <children>
            <Label layoutX="10.0" layoutY="9.0" prefHeight="44.0" prefWidth="110.0" style="-fx-font-size: 20;" text="SEQUENCE" />
            <Label layoutX="125.0" layoutY="10.0" prefHeight="44.0" prefWidth="55.0" style="-fx-font-size: 10;" text="SEQ Name:" />
            <TextField fx:id="sequenceName" layoutX="180.0" layoutY="21.0" prefHeight="10.0" prefWidth="80.0" promptText="Enter text" />
            <TextArea fx:id="sequenceQuery" layoutX="153.0" layoutY="100.0" prefHeight="351.0" prefWidth="222.0" promptText="query will be displayed" />
           	
               <CheckBox fx:id="incrementByCheckBox" layoutX="10.0" layoutY="130.0" text="INCREMENT BY" />
               <CheckBox fx:id="startWithCheckBox" layoutX="10.0" layoutY="160.0" text="START WITH" />
				<CheckBox fx:id="minValueCheckBox" layoutX="10.0" layoutY="190.0" text="MINVALUE" />
				<CheckBox fx:id="maxValueCheckBox" layoutX="10.0" layoutY="220.0" text="MAXVALUE" />
				<CheckBox fx:id="noCycleCheckBox" layoutX="10.0" layoutY="250.0" text="NOCYCLE" />
				<CheckBox fx:id="noCacheCheckBox" layoutX="10.0" layoutY="280.0" text="NOCACHE" />
				<CheckBox fx:id="noOrderCheckBox" layoutX="10.0" layoutY="310.0" text="NOORDER" />

            <Button layoutX="13.0" layoutY="369.0" mnemonicParsing="false" onAction="#generateSequenceQuery" prefHeight="22.0" prefWidth="108.0" text="생성" />
            <TextField fx:id="incrementByNum" layoutX="121.0" layoutY="127.0" prefHeight="22.0" prefWidth="24.0" />
            <TextField fx:id="startWithNum" layoutX="106.0" layoutY="157.0" prefHeight="22.0" prefWidth="24.0" />
            <TextField fx:id="minValueNum" layoutX="101.0" layoutY="187.0" prefHeight="22.0" prefWidth="48.0" />
            <TextField fx:id="maxValueNum" layoutX="101.0" layoutY="217.0" prefHeight="22.0" prefWidth="48.0" />
            <Button layoutX="13.0" layoutY="419.0" mnemonicParsing="false" onAction="#commitSequence" prefHeight="22.0" prefWidth="108.0" text="반영" />
           
               
          
               
               
         </children>
      </AnchorPane>
   </children>
</VBox>
